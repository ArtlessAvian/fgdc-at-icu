[application]

config/name="Fighting Game Development Club"
run/main_scene="res://Lobby/crude_connection.tscn"
config/icon="res://icon.png"

[autoload]

SyncManager="*res://addons/godot-rollback-netcode/SyncManager.gd"
SyncReplay="*res://addons/godot-rollback-netcode/SyncReplay.gd"
SyncDebugger="res://addons/godot-rollback-netcode/SyncDebugger.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-rollback-netcode/plugin.cfg" )

[input]

sync_debug={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777254,"physical_scancode":0,"unicode":0,"echo":false,"script":null)
 ]
}

[network]

rollback/log_inspector/replay_match_scene_path="res://Lobby/crude_connection.tscn"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
